{
  "name": "stage_name",
  "fetch_materials": false,
  "clean_working_directory": false,
  "never_cleanup_artifacts": false,
  "approval": {
    "type": "manual",
    "authorization": {
      "roles": [
        "two",
        "one"
      ]
    }
  },
  "jobs": [
    {
      "name": "job-name",
      "tasks": [
        {
          "type": "exec",
          "attributes": {
            "run_if": [
              "passed"
            ],
            "command": "/usr/local/bin/terraform",
            "working_directory": "/tmp/",
            "arguments": [
              "version",
              "-debug"
            ]
          }
        }
      ]
    }
  ]
}